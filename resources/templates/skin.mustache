
{{{html-headelement}}}
{{html-site-notice}}
<div class="row content-container">

<tuleap-project-sidebar config="{{tuleap-project-sidebar-config}}" {{#tuleap-project-sidebar-collapsed}}collapsed{{/tuleap-project-sidebar-collapsed}}></tuleap-project-sidebar>

<div class="main-container">
<div class="fixed-container">
	<div class="mw-tlp-dropdown">
	<button role="button" class="mw-tlp-fixed-btn">{{msg-tlp-personal-menu-title}}</button>
	<div class="mw-tlp-skin-btn-dropdown-menu">
		<ul>
		{{{personal-tools}}}
		</ul>
	</div>
	</div>
</div>
<div class="navbar">
	<div class="row">
		<nav>
			<div class="mw-tlp-dropdown">
				<a role="button" class="mw-tlp-skin-navbar-item" href="{{main-menu-href}}">{{msg-tlp-main-menu-title}}</a>
				<div class="mw-tlp-skin-btn-dropdown-menu">
					<ul>
					{{#sidebar}}
						<li><a id="{{id}}" href="{{href}}">{{text}}</a></li>
					{{/sidebar}}
					</ul>

				</div>
			</div>
		</nav>
	</div>
</div>
<div id="main-content" class="content">
	<div id="content" class="mw-body-primary" role="main">
		<div class="title-section">
			<h1 id="firstHeading" class="firstHeading" lang="en"><span dir="auto">{{html-title}}</span></h1>
			<div class="title-section-actions">
				{{#editaction}}
					<div class="title-section-spacer">
						<button role="button" class="mw-tlp-skin-btn mw-tlp-skin-btn-primary" ><a id="{{id}}" href="{{href}}">{{text}}</a></button>
					</div>
				{{/editaction}}
				<div class="mw-tlp-dropdown title-section-spacer">
					<button role="button" class="mw-tlp-skin-btn mw-tlp-skin-btn-secondary ">{{msg-tlp-tools-menu-title}}</button>
					<div class="mw-tlp-skin-btn-dropdown-menu">
						<ul>
						<li class="mw-tlp-skin-dropdown-title">{{msg-tlp-actions-menu-title}}</li>
						{{#actions}}
								<li><a id="{{id}}" href="{{href}}">{{text}}</a></li>
						{{/actions}}
						</ul>
						<ul>
							<li class="mw-tlp-separator"></li>
							<li class="mw-tlp-skin-dropdown-title">{{msg-tlp-tools-menu-title}}</li>
							{{{toolbox}}}
						</ul>
					</div>
				</div>

				<div class="title-section-actions-spacer"></div>
				{{#data-search-box}}
				<div class="mw-tlp-search">
					<form action="{{form-action}}" role="search" class="mw-portlet" id="p-search">
						<input type="hidden" name="title" value="{{page-title}}">
						{{{html-input}}}
					</form>
				</div>
				{{/data-search-box}}
			</div>
		</div>

		<div id="bodyContent" class="mw-body">
			<div id="siteSub"></div>
			<div id="contentSub">{{{html-subtitle}}}</div>
			<div id="contentSub2">{{{html-undelete-link}}}</div>
			<!-- start content -->
			<div id="mw-content-text" lang="en" dir="ltr" class="mw-content-ltr">
				{{{html-body-content}}}
			</div>
			{{html-after-content}}

			<div class="printfooter">
				{{{html-categories}}}
			</div>
		</div>
	<div class="content-end-spacer"></div>
	</div>

</div>